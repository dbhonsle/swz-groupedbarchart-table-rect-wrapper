{
    "name": "d3chartwithtablewrapper",
    "version": "0.4.7",
    "private": false,
    "description": "Grouped BarChart with Table UI Element",
    "author": "Swz (https://www.springwiz.com/)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "git@github.com:dbhonsle/swz-groupedbarchart-table-rect-wrapper.git"
    },
    "main": "lib/cjs/index.js",
    "module": "lib/index.js",
    "devtool": "source-map",
    "scripts": {
        "build": "yarn run clean && yarn run build-production && yarn run build-lib && yarn run build-cjs",
        "build-interactive": "yarn run clean && yarn run build-dev",
        "build-dev": "BABEL_ENV=dev NODE_ENV=dev webpack --progress --colors --mode development",
        "build-production": "BABEL_ENV=production NODE_ENV=production node_modules/.bin/webpack --mode production",
        "build-lib": "./node_modules/.bin/cpx './src/{**/!(__tests__)/*,*}' lib && BABEL_ENV=npm NODE_ENV=production ./node_modules/.bin/babel lib -d lib",
        "build-cjs": "./node_modules/.bin/cpx './src/{**/!(__tests__)/*,*}' lib/cjs && BABEL_ENV=cjs NODE_ENV=production ./node_modules/.bin/babel lib/cjs -d lib/cjs",
        "clean": "rm -rf dist && rm -rf lib",
        "styleguide-static": "REACT=true BABEL_ENV=production NODE_ENV=production node --max_old_space_size=8192 node_modules/react-styleguidist/bin/styleguidist.js build --config ./styleguide.config.js --mode production",
        "prettier": "prettier-eslint --print-width 120 --tab-width 4 --single-quote --semi --write --bracket-spacing true \"src/**/*.js\""
    },
    "dependencies": {
        "d3": "^5.7.0"
    },
    "devDependencies": {
        "autoprefixer": "^9.1.3",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-require-ignore": "^0.1.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cpx": "^1.5.0",
        "css-loader": "^0.28.7",
        "d3": "^5.7.0",
        "eslint": "^4.5.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.3.0",
        "eslint-watch": "^3.1.2",
        "mini-css-extract-plugin": "^0.4.2",
        "node-sass": "^4.5.3",
        "optimize-css-assets-webpack-plugin": "^5.0.0",
        "polished": "^1.7.0",
        "postcss-loader": "^3.0.0",
        "postcss-safe-parser": "^4.0.1",
        "prettier-eslint-cli": "^4.7.1",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-styleguidist": "^7.3.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "styled-components": "^2.1.2",
        "webpack": "^4.17.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.6"
    },
    "peerDependencies": {
        "react": "15 || 16",
        "react-dom": "15 || 16"
    }
}
